{
    "name": "all-dynamic-imports",
    "version": "0.0.1",
    "description": "TODO",
    "type": "module",
    "module": "./index.js",
    "typings": "./index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/ja0nz/ts-scratch.git",
        "directory": "packages/all-dynamic-imports"
    },
    "homepage": "https://github.com/ja0nz/ts-scratch/tree/main/packages/all-dynamic-imports#readme",
    "author": "Ja0nz <mail@ja.nz>",
    "license": "Apache-2.0",
    "publishConfig": {
        "registry": "http://localhost:4873"
    },
    "scripts": {
        "build": "yarn clean && tsc --declaration",
        "clean": "rimraf '*.js' '*.d.ts' '*.map' doc",
        "doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
        "pub": "yarn build && yarn npm publish",
        "test": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' testament test"
    },
    "dependencies": {
        "@thi.ng/api": "^8.3.2"
    },
