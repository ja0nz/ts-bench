        "@microsoft/api-extractor": "^7.18.19",
        "@thi.ng/testament": "^0.2.2",
        "rimraf": "^3.0.2",
        "typedoc": "^0.22.10",
        "typescript": "^4.5.2"
    },
    "keywords": [
        "typescript"
    ],
    "files": [
        "*.js",
        "*.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./index.js"
        }
    }
