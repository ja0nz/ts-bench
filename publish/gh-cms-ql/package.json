  "devDependencies": {
    "@microsoft/api-extractor": "^7.19.4",
    "@thi.ng/errors": "^2.1.4",
    "@thi.ng/testament": "^0.2.3",
    "rimraf": "^3.0.2",
    "tools": "workspace:^",
    "typescript": "^4.6.2"
