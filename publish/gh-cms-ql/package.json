    "github"
  ],
  "homepage": "https://github.com/ja0nz/ts-bench/tree/main/publish/gh-cms-ql#readme",
  "bugs": {
    "url": "https://github.com/ja0nz/ts-bench/issues"
  },
  "repository": {
    "type": "git",
